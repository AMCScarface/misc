#! /usr/bin/env stap

probe kernel.function("*@net/socket.c").call {
	if ((execname() != "Xorg") && (probefunc() != "sock_poll"))
		printf ("%s -> %s\n", thread_indent(1), probefunc())
}

probe kernel.function("*@net/socket.c").return {
	if ((execname() != "Xorg") && (probefunc() != "sock_poll"))
		printf ("%s <- %s\n", thread_indent(-1), probefunc())
}

